<!doctype html>
<html>
<head>
  <meta charset="utf-8"/>
  <title><%= source %> â€“ Free Intel Hub</title>
  <link rel="stylesheet" href="/style.css"/>
</head>
<body>
  <%- include("partials/navbar") %>

  <div class="container">
    <div class="section-title">Source: <%= source %></div>

    <div class="grid">
      <% rows.forEach(r => { %>
        <article class="card">
          <h4><a href="<%= r.link %>" target="_blank" rel="noopener"><%= r.title %></a></h4>
          <p><%= r.summary %></p>
          <div class="meta">
            <%= new Date(r.published_at).toLocaleString() %>
          </div>
        </article>
      <% }) %>
    </div>
  </div>

  <script src="/app.js"></script>
</body>
</html>
<%- include("partials/footer") %>
